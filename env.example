# NextAuth Configuration
NEXTAUTH_URL=https://appfactoryhub.vercel.app
NEXTAUTH_SECRET=defina_um_secret_seguro_aqui

# GitHub OAuth (para login)
GITHUB_ID=seu_github_client_id
GITHUB_SECRET=seu_github_client_secret

# GitHub API (para criar repos do template + escrever arquivos)
GITHUB_OWNER=cunhamarcela
GITHUB_TEMPLATE_REPO=app-factory-template
