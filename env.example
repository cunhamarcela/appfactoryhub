# Database (Postgres)
DATABASE_URL=postgresql://postgres:password@localhost:5432/app_factory_hub?schema=public

# Google OAuth (para Calendar)
GOOGLE_CLIENT_ID=seu_google_client_id
GOOGLE_CLIENT_SECRET=seu_google_client_secret
GOOGLE_REDIRECT_URI=https://appfactoryhub.vercel.app/api/auth/callback/google

# OpenAI (para agentes IA)
OPENAI_API_KEY=sk-seu_openai_api_ke
